  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "GatLab <dev@gatlab.co.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gatlab/discord-simple-webhook.git"
  },
  "bugs": {
    "url": "https://github.com/gatlab/discord-simple-webhook/issues"
  },
  "scripts": {
    "prestart": "yarn run build",
    "start": "node dist/example/example.js",
    "dev": "ts-node src/index.ts",
    "build": "SET NODE_ENV=production rimraf ./dist &&  tsc -p ./tsconfig.json",
    "test": "ts-node src/example/example.ts",
    "jest": "jest --coverage"
  },
  "files": [
    "/dist"
  ],
  "jest": {
    "collectCoverage": true,
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testRegex": "^.+\\.spec\\.ts$"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.1",
    "rimraf": "^4.1.2",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "axios": "^1.3.5",
    "form-data": "^4.0.0"
  }
